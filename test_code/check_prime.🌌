let number to check := 1001;

let prime check := 1;

if number < 2 {
    prime := 0;
} else {
    let i the potential divisor := 2;
    while i*i <= number {
        print("considering ");
        println(i);
        if number % i == 0 {
            prime := 0;
            break;
        }
        
        i := i + 1;
    }
}

if prime {
    print (number);
    println (" is prime");
} else {
    print (number);
    println (" is not prime");
}
