func is prime checks whether a given `number` is prime {
    let divisor count := 0;
    let i the potential divisor := 2;

    while i * i <= number {
        if number % i == 0 {
            divisor count := d + 1;
        }
        i := i + 1;
    }

    return number >= 2 && divisor count == 0;
}

let k the potential prime := 2;
while k < 100 {
    if is prime (k) {
        println(k);
    }

    k := k + 1;
}